  <meta name="theme-color" content="#fafafa">
  <style>
    body {
      font-family: -apple-system,BlinkMacSystemFont,"Segoe UI",Roboto,Oxygen-Sans,Ubuntu,Cantarell,"Helvetica Neue",sans-serif;
      --monospace-font: "Source Code", "Plex", "Fira Code", Consolas, "Andale Mono WT", "Andale Mono", "Lucida Console", "Lucida Sans Typewriter", "DejaVu Sans Mono", "Bitstream Vera Sans Mono", "Liberation Mono", "Nimbus Mono L", Monaco, "Courier New", Courier, monospace;
    }
    .color-chips {
      display: flex;
      flex-wrap: wrap;
    }
    .color-chip {
      position: relative;
      width: 200px;
      margin: 20px;
    }
    .color-chip-swatch {
      border-radius: 8px;
      padding: 8px;
      position: absolute;
      width: 100%;
      height: 100%;
      box-sizing: border-box;
      color: #fff
    }
    .color-chip-swatch h3,  .color-chip-swatch h4 {
      padding: 0;
      margin: 0;
      margin-left: -8px;
      margin-right: -8px;
      padding-left: 16px;
      padding-bottom: 4px;
      overflow-wrap: break-word;
    }
    .color-chip-swatch h3 {
      font-size: 1.2rem;
      font-weight: 700;
    }
    .color-chip-swatch h4 {
      font-size: 1rem;
      font-weight: normal;
      margin-bottom: 1.2rem;
      font-family: var(--monospace-font);
    }
    .color-chip-swatch.frame {
      border: 1px solid #ddd
    }
    .color-chip-swatch.light {
      color: #333;
    }
    .color-chip p {
      margin: 0;
      padding-top: .5rem;
      font-size: .86rem;
      line-height: 1.2;
      color: #777;
    }
    .color-chip-swatch-container {
      position: relative;
      width: 100%;
      min-height: 100px;

      border-radius: 8px;

      background-image: linear-gradient(45deg, #777 25%, transparent 25%), linear-gradient(-45deg, #777 25%, transparent 25%), linear-gradient(45deg, transparent 75%, #777 75%), linear-gradient(-45deg, transparent 75%, #777 75%);
      background-size: 20px 20px;
      background-position: 0 0, 0 10px, 10px -10px, -10px 0px;
    }
    .value-source {
      font-weight: bold;
      font-family: var(--monospace-font);
      overflow-wrap: break-word;
    }
  </style>
</head>

